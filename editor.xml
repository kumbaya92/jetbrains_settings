<application>
  <component name="DefaultFont">
    <option name="FONT_FAMILY" value="Consolas" />
    <option name="LINE_SPACING" value="0.9" />
  </component>
  <component name="EditorSettings">
    <option name="USE_SOFT_WRAPS" value="" />
    <option name="CUSTOM_SOFT_WRAP_INDENT" value="4" />
    <option name="IS_WHITESPACES_SHOWN" value="true" />
    <option name="IS_LEADING_WHITESPACES_SHOWN" value="false" />
    <option name="IS_INNER_WHITESPACES_SHOWN" value="false" />
    <option name="IS_ALL_SOFTWRAPS_SHOWN" value="true" />
    <option name="IS_ANIMATED_SCROLLING" value="false" />
    <option name="SHOW_PARAMETER_NAME_HINTS" value="false" />
  </component>
  <component name="LiveEditOptions" chromeUpdateApp="true" chromeUpdateOnChanges="true" restartIfHotSwapFailed="true">
    <option name="elementHighlighting" value="true" />
  </component>
  <component name="SqlEditorOptions">
    <option name="concatenateStringsOnEnter" value="true" />
    <option name="qualificationType" value="On collisions" />
  </component>
  <component name="TodoConfiguration">
    <pattern icon="question" useCustomColors="true" pattern="\bTODO:.*\b">
      <option name="FOREGROUND" value="c93b47" />
      <option name="BACKGROUND" value="b0b032" />
      <option name="FONT_TYPE" value="1" />
    </pattern>
    <pattern useCustomColors="true" pattern="\bFIXME:.*\b">
      <option name="FOREGROUND" value="c2d2b2" />
      <option name="BACKGROUND" value="c93b47" />
      <option name="FONT_TYPE" value="1" />
    </pattern>
    <pattern icon="important" useCustomColors="true" pattern="\bBACK:.*\b">
      <option name="FOREGROUND" value="1312c0" />
      <option name="BACKGROUND" value="c1c13b" />
      <option name="FONT_TYPE" value="1" />
      <option name="EFFECT_COLOR" value="c93b47" />
      <option name="ERROR_STRIPE_COLOR" value="368bc6" />
    </pattern>
    <filter name="FIXME">
      <pattern index="1" />
    </filter>
    <filter name="BACK">
      <pattern index="2" />
    </filter>
    <filter name="TODO">
      <pattern index="0" />
    </filter>
  </component>
  <component name="XmlEditorOptions">
    <option name="showCssInlineColorPreview" value="true" />
    <option name="tagTreeHighlightingLevelCount" value="4" />
    <option name="tagTreeHighlightingOpacity" value="20" />
  </component>
</application>